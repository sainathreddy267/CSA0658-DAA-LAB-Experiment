#include<stdio.h>
int main()
{
    int n,r,sum=0,temp;
    printf("enter an number:");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        sum=sum+(r*r*r);
        n=n/10;
    }
    if(temp==sum)
        printf("amstrong number");
    else
        printf("non amstrong number");
    return 0;
}a
